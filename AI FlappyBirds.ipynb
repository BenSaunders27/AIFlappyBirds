    "import random\n",
    "import os\n",
    "import pygame\n",
    "import operator\n",
    "import math\n",
    "def sig(x):\n",
    "    if x < 0:\n",
    "        return 1 - (1 / (1 + math.exp(x)))\n",
    "    else:\n",
    "        return 1 / (1 + math.exp(-x))\n",
    "pygame.init()\n",
    "pygame.font.init()\n",
    "\n",
    "def screen():\n",
    "    #Initial parameters\n",
    "    display_width = 800\n",
    "    display_height = 600\n",
    "\n",
    "    black = (0,0,0)\n",
    "    white = (255,255,255)\n",
    "    red = (255,0,0)\n",
    "    \n",
    "    screen = pygame.display.set_mode((display_width,display_height))\n",
    "    pygame.display.set_caption('Flappy Birds - AI')\n",
    "    clock = pygame.time.Clock()\n",
    "    return\n",
    "\n",
    "def set_random(birds, i0_rand_start, i12_rand_start, i3_rand_start, h_rand_start):\n",
    "    \n",
    "    weights_multiple = []\n",
    "    i0_rand = i0_rand_start\n",
    "    i12_rand = i12_rand_start\n",
    "    i3_rand = i3_rand_start\n",
    "    h_rand = h_rand_start\n",
    "    \n",
    "    for i in range(birds):\n",
    "        i0_h0 = random.uniform(-1,1)*i0_rand\n",
    "        i1_h0 = random.uniform(-1,1)*i12_rand\n",
    "        i2_h0 = random.uniform(-1,1)*i12_rand\n",
    "        i3_h0 = random.uniform(-1,1)*i3_rand\n",
    "\n",
    "        i0_h1 = random.uniform(-1,1)*i0_rand\n",
    "        i1_h1 = random.uniform(-1,1)*i12_rand\n",
    "        i2_h1 = random.uniform(-1,1)*i12_rand \n",
    "        i3_h1 = random.uniform(-1,1)*i3_rand\n",
    "\n",
    "        i0_h2 = random.uniform(-1,1)*i0_rand\n",
    "        i1_h2 = random.uniform(-1,1)*i12_rand\n",
    "        i2_h2 = random.uniform(-1,1)*i12_rand\n",
    "        i3_h2 = random.uniform(-1,1)*i3_rand\n",
    "\n",
    "        h0_out = random.uniform(-1,1)*h_rand \n",
    "        h1_out = random.uniform(-1,1)*h_rand\n",
    "        h2_out = random.uniform(-1,1)*h_rand \n",
    "\n",
    "        weights = [[[i0_h0,i1_h0,i2_h0,i3_h0],[i0_h1,i1_h1,i2_h1,i3_h1],[i0_h2,i1_h2,i2_h2,i3_h2]],[h0_out,h1_out,h2_out]]\n",
    "        \n",
    "        weights_multiple.append(weights)\n",
    "    \n",
    "    return weights_multiple\n",
    "\n",
    "def break_weights(weights):\n",
    "    i0_h0 = weights[0][0][0]\n",
    "    i1_h0 = weights[0][0][1]\n",
    "    i2_h0 = weights[0][0][2]\n",
    "    i3_h0 = weights[0][0][3]\n",
    "\n",
    "    i0_h1 = weights[0][1][0]\n",
    "    i1_h1 = weights[0][1][1]\n",
    "    i2_h1 = weights[0][1][2]\n",
    "    i3_h1 = weights[0][1][3]\n",
    "\n",
    "    i0_h2 = weights[0][2][0]\n",
    "    i1_h2 = weights[0][2][1]\n",
    "    i2_h2 = weights[0][2][2]\n",
    "    i3_h2 = weights[0][2][3]\n",
    "\n",
    "    h0_out = weights[1][0]\n",
    "    h1_out = weights[1][1]\n",
    "    h2_out = weights[1][2]\n",
    "    return i0_h0,i1_h0,i2_h0,i3_h0 , i0_h1,i1_h1,i2_h1,i3_h1 , i0_h2,i1_h2,i2_h2,i3_h2 , h0_out,h1_out,h2_out\n",
    "\n",
    "def neural(x_dist,y_top_dist,y_bot_dist,velocity,weights):\n",
    "    i0_h0,i1_h0,i2_h0,i3_h0 , i0_h1,i1_h1,i2_h1,i3_h1 , i0_h2,i1_h2,i2_h2,i3_h2 , h0_out,h1_out,h2_out = break_weights(weights)\n",
    "    i0 = x_dist\n",
    "    i1 = y_top_dist \n",
    "    i2 = y_bot_dist\n",
    "    i3 = velocity\n",
    "    h0 = sig( (i0*i0_h0) + (i1*i1_h0) + (i2*i2_h0) + (i3*i3_h0) )\n",
    "    h1 = sig( (i0*i0_h1) + (i1*i1_h1) + (i2*i2_h1) + (i3*i3_h1) )\n",
    "    h2 = sig( (i0*i0_h2) + (i1*i1_h2) + (i2*i2_h2) + (i3*i3_h2) )\n",
    "    out = sig( (h0*h0_out) + (h1*h1_out) + (h2*h2_out) )\n",
    "    return out\n",
    "\n",
    "def store_weights(status,weights_multiple):\n",
    "    filename = 'C:\\\\Users\\\\b.saunders\\\\Documents\\\\Python\\\\Projects\\\\Flappy Birds\\\\weights.csv'\n",
    "    \n",
    "    if status == \"old\":\n",
    "        file = open(filename,'r')\n",
    "        read = file.read()\n",
    "        file.close()\n",
    "    if status == \"new\":\n",
    "        read = \"\"\n",
    "    \n",
    "    for weights in weights_multiple:\n",
    "        i0_h0,i1_h0,i2_h0,i3_h0 , i0_h1,i1_h1,i2_h1,i3_h1 , i0_h2,i1_h2,i2_h2,i3_h2 , h0_out,h1_out,h2_out = break_weights(weights)\n",
    "        h0_w = str(i0_h0) + \",\" + str(i1_h0) + \",\" + str(i2_h0) + \",\" + str(i3_h0) + \",\"\n",
    "        h1_w = str(i0_h1) + \",\" + str(i1_h1) + \",\" + str(i2_h1) + \",\" + str(i3_h1) + \",\"\n",
    "        h2_w = str(i0_h2) + \",\" + str(i1_h2) + \",\" + str(i2_h2) + \",\" + str(i3_h2) + \",\"\n",
    "        out_w = str(h0_out) + \",\" + str(h1_out) + \",\" + str(h2_out) + \"\\n\"\n",
    "        read += h0_w + h1_w + h2_w + out_w\n",
    "\n",
    "    file_read = open(filename,'w')\n",
    "    file_read.write(read)\n",
    "    file_read.close()\n",
    "    \n",
    "    return\n",
    "\n",
